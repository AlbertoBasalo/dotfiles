{
  "cSpell.customDictionaries": {
    "word-up": {
      "addWords": true,
      "description": "Personal dictionary",
      "name": "personal",
      "scope": "user",
      "path": "~/OneDrive/personal-dictionary.txt"
    }
  },
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.language": "en,es",
  "debug.console.fontSize": 16,
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "always",
  "editor.autoIndent": "full",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": true,
    "source.fixAll": true,
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Fira Code', 'Cascadia Mono PL', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": true,
  "editor.hover.above": false,
  "editor.hover.sticky": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "comments": "off",
    "other": "inline",
    "strings": "off"
  },
  "editor.quickSuggestionsDelay": 1000,
  "editor.renderWhitespace": "none",
  "editor.semanticHighlighting.enabled": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Control flow",
        "scope": "keyword.control, keyword.control.flow",
        "settings": {
          "fontStyle": "italic bold"
        }
      },
      {
        "name": "Definitions",
        "scope": [
          "entity.name.type.class",
          "meta.definition.method",
          "meta.definition.property",
          "meta.definition.function",
          "entity.name.section.markdown"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Function/Method Calls HTML attributes",
        "scope": [
          "meta.function-call",
          "keyword.operator.new.ts",
          "entity.other.attribute-name.html"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "Other",
        "scope": [
          "keyword.control.import",
          "keyword.control.export",
          "keyword.control.from",
          "keyword.operator",
          "meta.type.annotation",
          "storage"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "emmet.triggerExpansionOnTab": true,
  "eslint.enable": true,
  "eslint.run": "onType",
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "node_modules/": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "dist/": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": true
  },
  "grammarly.config.documentDomain": "business",
  "html.format.enable": false,
  "html.format.indentInnerHtml": true,
  "html.format.wrapAttributes": "auto",
  "html.format.wrapLineLength": 120,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "material-icon-theme.files.associations": {
    "*.api.ts": "database",
    "*.block.ts": "angular-component",
    "*.component.ts": "angular",
    "*.config.ts": "tune",
    "*.control.ts": "angular",
    "*.dialog.ts": "angular-component",
    "*.dto.ts": "raml",
    "*.entity.ts": "raml",
    "*.enum.ts": "tune",
    "*.environment.ts": "tune",
    "*.factory.ts": "hardhat",
    "*.form.ts": "angular",
    "*.interceptor.ts": "angular-service",
    "*.interface.ts": "raml",
    "*.list.ts": "angular",
    "*.model.ts": "raml",
    "*.page.ts": "angular-resolver",
    "*.providers.ts": "angular-service",
    "*.repository.ts": "database",
    "*.store.ts": "redux-store",
    "*.storage.ts": "database",
    "*.table.ts": "angular",
    "*.transform.ts": "document",
    "transform.ts": "document",
    "*.type.ts": "raml",
    "validation.ts": "document",
    "*.validation.ts": "document",
    "*.view.ts": "angular",
    "CLI.md": "console",
    "TASKS.md": "todo"
  },
  "material-icon-theme.folders.associations": {
    "domain": "core",
    "state": "redux-store",
    "ui": "angular"
  },
  "prettier.bracketSameLine": true,
  "prettier.htmlWhitespaceSensitivity": "strict",
  "prettier.requireConfig": false,
  "screencastMode.fontSize": 28,
  "screencastMode.keyboardOverlayTimeout": 500,
  "screencastMode.keyboardShortcutsFormat": "command",
  "screencastMode.mouseIndicatorColor": "#3fb950",
  "screencastMode.mouseIndicatorSize": 36,
  "screencastMode.onlyKeyboardShortcuts": true,
  "search.exclude": {
    "**/env": true,
    "node_modules/": true
  },
  "terminal.integrated.fontSize": 16,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#3fb950",
    "editor.lineHighlightBorder": "#000000",
    "editorBracketHighlight.foreground1": "#a371f7",
    "editorBracketHighlight.foreground2": "#58a6ff",
    "editorBracketHighlight.foreground3": "#3fb950",
    "editorBracketHighlight.foreground4": "#d29922",
    "editorBracketHighlight.foreground5": "#db6d28",
    "editorBracketHighlight.foreground6": "#f85149",
    "editorError.foreground": "#f85149",
    "editorWarning.foreground": "#d29922",
    "tab.activeBorder": "#3fb950",
    "tab.activeBorderTop": "#3fb950",
    "terminalCursor.foreground": "#3fb950"
  },
  "workbench.colorTheme": "Winter is Coming (Light - No Italics)",
  "workbench.iconTheme": "material-icon-theme",
  "window.zoomLevel": 1
}
